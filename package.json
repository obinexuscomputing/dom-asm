{
  "name": "@obinexuscomputing/dom-asm",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "A unified library for DOM manipulation and compilation.",
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "clean": "turbo clean && rimraf node_modules packages/*/node_modules",
    "prebuild": "rimraf packages/*/dist",
    "pretest": "npm run build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vue/compiler-sfc": "^3.5.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "turbo": "^1.9.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@obinexuscomputing/dom-css": "*",
    "@obinexuscomputing/dom-html": "*",
    "@obinexuscomputing/dom-js": "*",
    "commander": "^11.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obinexuscomputing/dom-asm.git"
  },
  "bugs": {
    "url": "https://github.com/obinexuscomputing/dom-asm/issues"
  },
  "homepage": "https://github.com/obinexuscomputing/dom-asm#readme",
  "keywords": [
    "dom",
    "ast",
    "parser",
    "compiler",
    "html",
    "css",
    "javascript",
    "typescript"
  ],
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "asm": "./bin/asm",
    "asm.bat": "./bin/asm.bat",
    "asm.ps1": "./bin/asm.ps1"
  }
}